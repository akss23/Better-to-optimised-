class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int n=prices.size();
        int pr=0;
        int i=0;int j=1;
        while(j<n){
            if(prices[j]>prices[i]) {
              pr=pr+(prices[j]-prices[i]);
            }
            i++;
            j++;
        }
        return pr;
    }
};
